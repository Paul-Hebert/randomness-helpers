{
  "name": "seeded-helpers",
  "type": "module",
  "version": "0.0.1",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "run-p start:*",
    "build": "run-s build:*",
    "start:lib": "vite",
    "build:lib": "vite build",
    "start:docs": "npx @11ty/eleventy --serve",
    "build:docs": "npx @11ty/eleventy",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "random-seed": "^0.3.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-plugin-webc": "^0.9.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "@types/node": "^18.14.1",
    "@vitest/coverage-c8": "^0.29.1",
    "astro": "^2.0.14",
    "nodemon": "^2.0.20",
    "vite-plugin-dts": "^2.0.2",
    "vitest": "^0.29.1"
  }
}
